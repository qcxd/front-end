/* pages/my-home/my-home.wxss */

.myHome .home-head {
  padding: 15px;
  color: #ffffff;
  background-color: #35A1FB;
}

.myHome .home-head .head-img {
  width: 60px;
  height: 60px;
  margin-right: 15px;
  border-radius: 100px;
  border: 3px solid #ffffff;
}

.myHome .home-center {
  align-items: center;
  justify-content: space-around;
  padding: 15px 0px;
  background-color: #ffffff;
  text-align: center;
}

.myHome .small-text {
  padding: 2px;
  font-size: 13px;
  color: #4A4A4A;
  padding-left: 5px;
}

.myHome .home-cell {
  margin-top: 10px;
  padding: 10px 15px;
  background-color: #ffffff;
}

.myHome .home-cell-list {
  padding: 10px 15px;
  background-color: #ffffff;
}

.myHome .home-cell-list + .home-cell-list {
  padding: 10px 15px;
  background-color: #ffffff;
  border-top: 1px solid #efefef;
}

.myHome .marginTop_10 {
  margin-top: 10px;
}
