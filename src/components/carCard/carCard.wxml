<!--components/carCard/carCard.wxml-->
<view class='car-card'>
  <view>
    <!-- <view class='car-card-price'>{{carDatail.price}}万</view> -->
    <image src="{{carDatail.indexImage}}" mode='widthFix'></image>
  </view>
  <view class='car-card-price'>{{carDatail.price}}万</view>
  <view class='car-card-info'>
    <view>{{carDatail.brand}}</view>
    <view class='small-text'>2014年/88.98万公里</view>
  </view>
</view>
