/* components/phoneCall/phoneCall.wxss */

.phoneCall {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(49,41,41,0.85);
  text-align: center;
}
 .phoneCall .action-area {
  position: fixed;
  bottom: 0px;
  width:100%;
  background:#efefef;
 }
.phoneCall .phone {
  color: #ACB4B9;
  border-bottom: 1px solid #efefef;
  padding:10px;
  background:#ffffff;
}

.phoneCall .call {
  margin-bottom: 10px;
  padding:10px;
  background:#ffffff;
}


.phoneCall .cancel {
  padding:10px;
  background:#ffffff;
}